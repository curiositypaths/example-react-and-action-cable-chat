# Ignore bundler config.
/api/.bundle

# Ignore the default SQLite database.
/api/db/*.sqlite3
/api/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/api/log/*
/api/tmp/*
!/log/.keep
!/tmp/.keep

.byebug_history


# Frontend ignore directives
# dependencies
/frontend/node_modules

# testing
/frontend/coverage

# production
/frontend/build

# misc
.DS_Store
.env.local
.env.development.local
.env.test.local
.env.production.local

npm-debug.log*
yarn-debug.log*
yarn-error.log*
